@*@model MVCApplication.Models.Employee
@Model IEnumerator<MVCApplication.Models.Employee>*@
@model List<MVCApplication.Models.Employee>

@{
    Layout = null;
}
    <style>
        table th {
            border: 1px solid black;
            text-align: center
        }

        table td {
            border: 1px solid black;
            text-align: center
        }
    </style>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetEmployeeDetailsTable</title>
</head>
<body>
    <h4>Employees</h4>
    <hr />
    <table class="table table-bordered table-responsive table-hover" cellpadding="0" cellspacing="0">
        <tr>
            <th>Emp_id</th>
            <th>Emp_name</th>
            <th>Emp_mailid</th>
            <th>Emp_salary</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Emp_id</td>
                <td>@item.Emp_name</td>
                <td>@item.Emp_mailid</td>
                <td>@item.Emp_salary</td>
            </tr>
        }
    </table>
</body>
</html>
